{
  "name": "@ikrbasak/rate-limiter",
  "private": false,
  "version": "0.0.2",
  "description": "Lightweight, pluggable, and production-ready rate limiter for Node.js. Supports fine-grained control over rate limiting strategies (fixed window, sliding window, token bucket).",
  "keywords": [
    "rate-limiter",
    "token-bucket",
    "fixed-window",
    "sliding-window"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ikrbasak/rate-limiter#readme",
  "bugs": {
    "url": "https://github.com/ikrbasak/rate-limiter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ikrbasak/rate-limiter.git"
  },
  "license": "MIT",
  "author": "Krishna Basak <ikrbasak@gmail.com>",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "typecheck": "tsc",
    "bundle": "tsup",
    "test": "tsx node_modules/vitest/vitest.mjs run --cache=false",
    "test:coverage": "tsx node_modules/vitest/vitest.mjs run --coverage --cache=false",
    "prettier:fix": "prettier -w .",
    "prettier:check": "prettier -c .",
    "eslint:check": "eslint",
    "eslint:fix": "eslint --fix",
    "prepare": "husky || true"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@swc/core": "^1.13.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^24.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "validate-branch-name": "^1.3.2"
  },
  "peerDependencies": {
    "express": "^5.1.0",
    "hono": "^4.8.12",
    "ioredis": "^5.7.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "ioredis": {
      "optional": false
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier -c --ignore-unknown"
    ],
    "*.ts": [
      "eslint"
    ]
  }
}
